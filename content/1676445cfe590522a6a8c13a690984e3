                    <a href="/index.php?title=Special:Search&amp;go=Go&amp;search=PRIM_TEXTURE"><span style="color: #0000A0;font-weight: bold; text-decoration: none;">PRIM_TEXTURE</span></a>, <span style="color: #cc66cc;">3</span>, <span style="color: #00A000;">&quot;09b04244-9569-d21f-6de0-4bbcf5552222&quot;</span>, <span style="color: #66cc66;">&lt;</span><span style="color: #cc66cc;">0.126</span>, <span style="color: #cc66cc;">0.10</span>, <span style="color: #cc66cc;">0.0</span><span style="color: #66cc66;">&gt;</span>, <span style="color: #66cc66;">&lt;</span><span style="color: #cc66cc;">-0.255989</span>, <span style="color: #cc66cc;">0.0</span>, <span style="color: #cc66cc;">0.0</span><span style="color: #66cc66;">&gt;</span>, <span style="color: #cc66cc;">0.0</span><span style="color: #66cc66;">&#93;</span><span style="color: #66cc66;">&#41;</span>;
