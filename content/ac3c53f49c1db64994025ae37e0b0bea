//function for login 
function fnLogin()
{
//var s="PHPSESSID=a7296272500002b94b29ec3e174ca50f";
lolhost="0";
if(lolhost!=1)
path='http://www.learnoutloud.com'+"/";
else
path='http://teach.learnoutloud.com'+"/";
if(path=="./")
path="./";
 //window.open(path+'ulogin.php?'+s+'&nav=L','','scrollbars=yes,resizable=yes,width=400,height=300,TOP=140,LEFT=300')
 window.open(path+'ulogin.php?nav=L','','scrollbars=yes,resizable=yes,width=400,height=300,TOP=140,LEFT=300')
}
function fnlogout()
{
	document.frm.actions.value='logout';
	document.frm.submit();
}
</script>
