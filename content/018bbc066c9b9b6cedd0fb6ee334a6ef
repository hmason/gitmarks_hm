  var newspan = $('<span class="below"></span>'); 
  newspan.appendTo(".post h3");
  newspan.appendTo("h3.c_style");
  // Form Value Replace
	$('input').each(function () {
		if ($(this).val() == '') {
			$(this).val($(this).attr('title'));
		}
	}).focus(function () {
		$(this).removeClass('inputerror');
		if ($(this).val() == $(this).attr('title')) {
			$(this).val('');
		}
	}).blur(function () {
		if ($(this).val() == '') {
			$(this).val($(this).attr('title'));
		}
	});
});
// Preload images
if (document.images)
  {
    preload_image_object = new Image();
    // set image url
    image_url = new Array();
    image_url[0] = "http://freelanceswitch.com/wp-content/themes/fsw-v3/images/backgrounds/panel_large.jpg";
    image_url[1] = "http://freelanceswitch.com/wp-content/themes/fsw-v3/images/backgrounds/panel_small.gif";

     var i = 0;
     for(i=0; i<=1; i++) 
       preload_image_object.src = image_url[i];
