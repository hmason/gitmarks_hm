					$(document).ready(function(){
						var className;
						$("#image-map area").mouseover(function(){
						  var className = this.className; 
						  $(this).parents("#image-map-wrp").addClass(className); 
						}).mouseout(function(){
						  $("#image-map-wrp").removeClass(className);
						});
					
					});
