<script language=javascript type="text/javascript">
function showNavTab(tabId,tabNo) {
	var tabCollection = document.getElementById(tabId);
	tabCollection.className='Tab'+tabNo+'Visible';
}
function fsbAskQuestionVerifyShort(evalForm) {
	if ((evalForm.EMAIL.value == "")||(evalForm.EMAIL.value == "Your E-mail Address")) {
		alert("Please enter your email address.");
		evalForm.EMAIL.focus();
		return false;
	}
	if ((evalForm.CITY.value == "")||(evalForm.CITY.value == "Your City")) {
		alert("Please enter your city.");
		evalForm.CITY.focus();
		return false;
	}
	if (evalForm.STATE.value == "") {
		alert("Please choose your state.");
		evalForm.STATE.focus();
		return false;
	}
	if ((evalForm.PHONE.value == "")||(evalForm.PHONE.value == "Your Phone Number")) {
		alert("Please enter your phone number.");
		evalForm.PHONE.focus();
		return false;
	}
}
function fsbQuestionNext() {
	var questionArea = document.getElementById('yourQuestion');
	if ((questionArea.value == '')||(questionArea.value == 'Ask your small business question...')) {
		alert("Please enter a question.");
		questionArea.focus();
	} else {
		var stage1Area = document.getElementById('askQstage1');
		var stage2Area = document.getElementById('askQstage2');
		stage1Area.style.display = 'none';
		stage2Area.style.display = 'block';
	}
}
function fsbAskQuestionVerify(evalForm) {
	if ((evalForm.EMAIL.value == "")||(evalForm.EMAIL.value == "Your E-mail Address")) {
		alert("Please enter your email address.");
		evalForm.EMAIL.focus();
		return false;
	}
	if ((evalForm.CITY.value == "")||(evalForm.CITY.value == "Your City")) {
		alert("Please enter your city.");
		evalForm.CITY.focus();
		return false;
	}
	if (evalForm.STATE.value == "") {
		alert("Please choose your state.");
		evalForm.STATE.focus();
		return false;
	}
	if ((evalForm.PHONE.value == "")||(evalForm.PHONE.value == "Your Phone Number")) {
		alert("Please enter your phone number.");
		evalForm.PHONE2.focus();
		return false;
	}
	if ((evalForm.QUESTIONS.value == "")||(evalForm.QUESTIONS.value == "Ask your small business question...")) {
		alert("Please enter a question.");
		evalForm.QUESTIONS.focus();
		return false;
	}
}
</script>
