<script type="text/javascript">
updateUserCenter("", "", "");
function showNotice(msg) {
	$(".notifications_content").html(msg);
	$(".notifications").slideDown("slow", function(){
		setTimeout(hideNotice, 5000);
	});
}
function hideNotice() {
	$(".notifications").slideUp("slow", function(){
		$(".notifications_content").html("");
	});	
}
/*
$(".uc_icon").mouseover(function(){
	var drop = $(".uc_drop");
	var offset = drop.offset();
	var tt = offset.top + drop.height() - 7;
	var tl = offset.left;
	var tw = drop.width() - 2;
	$(".uc_drop_list").css({"top":tt+"px", "left":tl+"px", "width":tw+"px"}).show();
});


$(".uc_drop").mouseout(function(){
	$(".uc_drop_list").hide();												
});*/

$(".uc_drop").hover(function(){
    var drop = $(this);
    var offset = drop.offset();
    var tt = offset.top + drop.height() - 7;
    var tl = offset.left;
    var tw = drop.width() - 2;
    $(".uc_drop_list").css({"top":tt+"px", "left":tl+"px", "width":tw+"px"}).show();
},function(){
    $(".uc_drop_list").hide();                                                
});
