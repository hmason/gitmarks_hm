<script type="text/javascript">
	var pageTracker = _gat._getTracker("UA-2685675-4");
	function gaTrack(pID){
		try {
			pageTracker._trackPageview(pID);
		} catch(err) {}
			
	}
	
	function trackSale(pID, pPlan, pName, pTotal, pTax, pCity, pCountry){
		 pageTracker._addTrans(
			pID, // Order ID
			"", // Affiliation
			pTotal, // Total
			pTax, // Tax
			"", // Shipping
			pCity, // City
			"", // State
			pCountry // Country
			);

		pageTracker._addItem(
			pID, // Order ID
			pPlan, // SKU
			pName, // Product Name
			"", // Category
			pTotal, // Price
			"1" // Quantity
			); 
	}

</script>

