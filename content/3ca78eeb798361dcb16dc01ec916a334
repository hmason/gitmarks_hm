<script type="text/javascript" charset="utf-8">
	function validate(){
		var elem = document.getElementById('homeinput') || document.getElementById("searchbox");
		if(elem.value == null || elem.value=="" || elem.value.replace(/\s+/gi,'') == '') {return false;}
	}
</script>
