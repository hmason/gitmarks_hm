<script type="text/javascript" language="JavaScript">
function processClick(location) {
window.open(location);
}
var google_afd_request = {
  client: 'ca-dp-godaddy2_xml',  domain_name: 'www.escapepod.info',
  hl: 'en',
  num_radlinks: '0',
  s: 'escapepod.info'
}

var handler = '/ct.html?';
var handler2 = 'http://escapepod.info';
var ads = '';
var popularCategories = '';
var relatedSearches = '';
var kotValue = '';

var loc1 = handler2 + '?3d5dc8f0ea460ea4eed1cc5a4cc709f4=325467165&6fa970d50480e816942b6fab7c4e7a23=506683626&5f17a83ba43604eb3d1c06f73d82e119=634284330168267838';
var loc2 = handler2 + '?0f41fdd8c9af62a4027fda82d597062a=1623525913&3d5dc8f0ea460ea4eed1cc5a4cc709f4=2344573&f67ff3454420ebb255fe4f673cf8cf0b=AwZ0Zwt0ZmZjZGL4ZwL3BQV1-1';

function google_afd_ad_request_done(google_afd_response) {
   var google_ads = google_afd_response.ads;

   if (google_afd_response.faillisted) {
       window.location.href = loc1;
       return;
   }

   if (google_afd_response.needsreview) {
       window.location.href = loc2;
       return;
   }

   if (google_ads.length > 0) {

      ads += '<div style="width: 435px;"><div style="font-size: 14px; font-weight: bold; color: #000; padding-left: 10px; padding-top:5px; padding-bottom: 5px;">';

      if (google_afd_response.feedback_url > '' && '1' == '1') {
          ads += '<a style="text-decoration:none;color:#000;" TARGET="_blank" href="' + google_afd_response.feedback_url + '" >Sponsored Listings</a>';
      }
      else{
          ads += 'Sponsored Listings';
      }
      
      ads += '</div><div style="background-color: #ffffff; ">';

      for (var i = 0; i < google_ads.length; i++) {
         var onclickScript = '';

                    
            onclickScript = 'onclick="processClick(\'' + handler + 'u=' + encodeURIComponent(google_ads[i].url) +'&e=Wzp9ZwRjZvMxCFMwCGNznG0zMG0zow0zL3MkCGxkBQH3ZQDjZGZkZwN5AGZ0ZQtzMJp9ZwNkZQRlZwNjBGRjZGLzL3x9ZFM4CFM0Mm0lWzMapQ0jWzuhMm0kWaOjCHuTWaEzCGLzpUZ9BGR4AGpjAQNkZmRlZQx1ZmDjBN==-1\'); return false;"';
         

         ads += '<div class="adblock">';

          
         ads += '<a class="linkhd" style="text-decoration:underline;color:#00F;cursor:pointer;" title="'+ google_ads[i].visible_url +'" ' + onclickScript + '>'+ google_ads[i].line1 +'</a>';
         
         ads += '<div>'+ google_ads[i].line2;
         
		 if (google_ads[i].line3 != undefined && google_ads[i].line3 != '') { ads += '<br/>' + google_ads[i].line3;}
		 ads += '</div>';

          
         ads += '<a class="sponsorurl" style="cursor:pointer;"  title="' + google_ads[i].visible_url + '" ' + onclickScript + '>' + google_ads[i].visible_url + '</a>';
         
         ads += '</div><div style="clear: both;"></div>'; }

     ads += '</div></div>';
   }

   if (google_afd_response.search_token)
   {
      kotValue = '<input type="hidden" name="kot" value="' + google_afd_response.search_token +'"/>';
   }

}
