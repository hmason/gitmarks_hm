<table width='120' align='left' border='0' cellpadding='0' cellspacing='0' style='padding:0 8px 0 0'><tr><td><a onclick='javascript:window.open("render_graphic.php?&type=graphic_in_html&title=Popup&filename=../sites/nrsi/media/xetex_unicode_amharic.png", "Popup", "toolbar=0,location=0,status=1,menubar=0,scrollbars=1,resizable=1,width=987,height=1017");'><img src='render_graphic.php?&type=media_graphic&filename=../sites/nrsi/media/xetex_unicode_amharic.png&image_type=png&width=100&height=102' width='100' height='102' border='0'/><img src='../assets/icons/magnify.gif' border='0'></a></td></tr><tr style='padding:0 8px 0 0'><td style='padding:0 8px 0 0'><p class='Caption'>Amharic Unicode text typeset by XeTeX</p></td></tr></table><table width='120' align='left' border='0' cellpadding='0' cellspacing='0' style='padding:0 8px 0 0'><tr><td><a onclick='javascript:window.open("render_graphic.php?&type=graphic_in_html&title=Popup&filename=../sites/nrsi/media/xetex_unicode_arabic.png", "Popup", "toolbar=0,location=0,status=1,menubar=0,scrollbars=1,resizable=1,width=1043,height=913");'><img src='render_graphic.php?&type=media_graphic&filename=../sites/nrsi/media/xetex_unicode_arabic.png&image_type=png&width=100&height=86' width='100' height='86' border='0'/><img src='../assets/icons/magnify.gif' border='0'></a></td></tr><tr style='padding:0 8px 0 0'><td style='padding:0 8px 0 0'><p class='Caption'>Typesetting an Arabic Unicode document</p></td></tr></table><table width='120' align='left' border='0' cellpadding='0' cellspacing='0' style='padding:0 8px 0 0'><tr><td><a onclick='javascript:window.open("render_graphic.php?&type=graphic_in_html&title=Popup&filename=../sites/nrsi/media/xetex_unicode_extlatin.png", "Popup", "toolbar=0,location=0,status=1,menubar=0,scrollbars=1,resizable=1,width=1201,height=998");'><img src='render_graphic.php?&type=media_graphic&filename=../sites/nrsi/media/xetex_unicode_extlatin.png&image_type=png&width=100&height=81' width='100' height='81' border='0