var parent_guid = video_guid;

function getYTLink(id,user_name,tweet) {
	var url = "/index.php?area=ajax&cmd=getYTVideoGUID";
	$.get(url, {yt_id:id, yt_tweet_user:user_name, yt_tweet_msg:tweet}, function(data){
		//alert(data);
		window.location = "http://www.twitvid.com/"+data;
		
	});
}

</script>
