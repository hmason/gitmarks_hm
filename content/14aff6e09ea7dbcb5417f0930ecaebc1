<script type="text/javascript">
var initSizeSlider = function() {
	slide_size = new Control.Slider(['size_slider_min', 'size_slider_max'], 'size_slider', {
	    range: $R(0, 4),
	    values: $R(0, 4),
	    sliderValue: [0, 4],
	    restricted: true,
	    onChange: function(values) {
	    	var lookup = new Array(747,748,749,750,751);
	    	$('size_from').value = lookup[values[0]];
	    	$('size_to').value = lookup[values[1]];
	  		document.fire('layerednav:submit');
	    }
	});
}

document.observe('layerednav:load', initSizeSlider);
document.observe('layerednav:cleared', initSizeSlider);
document.observe('layerednav:disable', function() { slide_size.setDisabled(); });
document.observe('layerednav:enable', function() { slide_size.setEnabled(); });
</script><dt class="code-color_basic" code="color_basic">
