<script type="text/javascript">
onepica.Search = Class.create({
	initialize: function(form, input) {
		this.form = form;
		this.input = input;
		
		this.input.writeAttribute('autocomplete', 'off');
		this.input.writeAttribute('placeholder', 'Search...');
		if(this.input.value == '') this.reset();
		
		this.input.observe('focus', function() {
			if(this.input.value == this.input.readAttribute('placeholder')) {
				this.input.removeClassName('blur');
				this.input.value = '';
			}
		}.bind(this));
		
		this.input.observe('blur', function() {
			if(this.input.value == '') this.reset();
		}.bind(this));
		
		this.form.observe('submit', function(event) {
			if(this.input.value == '' || this.input.value == this.input.readAttribute('placeholder')) {
				Event.stop(event);
				return;
			}
						document.fire('layerednav:submit', { q:this.input.value.escapeHTML() });
			Event.stop(event);
					}.bindAsEventListener(this));
		
		document.observe('onepica:product-list-updated', function(event) {
			this.reset();
		}.bindAsEventListener(this));
	},
	
	reset: function() {
		this.input.addClassName('blur');
		this.input.value = this.input.readAttribute('placeholder');
	}
});

new onepica.Search($('search-wrapper'), $('q'));
</script><ul class="links">
