ResetCharIndex<span style="color: #66cc66;">&#40;</span><span style="color: #66cc66;">&#41;</span> <span style="color: #66cc66;">&#123;</span> 
    gCharIndex  <span style="color: #66cc66;">=</span> <span style="color: #00A000;">&quot;&nbsp;!<span style="color: #FF7700;">\&quot;</span>#$%&amp;'()*+,-./0123456789:;&lt;=&gt;?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[<span style="color: #FF7700;">\\</span>]^_`&quot;</span>; 
    <span style="color: #FF7700;">// \&quot; &lt;-- Fixes LSL syntax highlighting bug. </span>
    gCharIndex <span style="color: #66cc66;">+=</span> <span style="color: #00A000;">&quot;abcdefghijklmnopqrstuvwxyz{|}~&quot;</span>; 
    gCharIndex <span style="color: #66cc66;">+=</span> <span style="color: #00A000;">&quot;<span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span>&quot;</span>; 
<span style="color: #66cc66;">&#125;</span> 
&nbsp;
<a href="/index.php?title=vector"><span style="color: #007700;font-weight: bold; text-decoration: none;">vector</span></a> GetGridPos<span style="color: #66cc66;">&#40;</span><a href="/index.php?title=integer"><span style="color: #007700;font-weight: bold; text-decoration: none;">integer</span></a> index1, <a href="/index.php?title=integer"><span style="color: #007700;font-weight: bold; text-decoration: none;">integer</span></a> index2<span style="color: #66cc66;">&#41;</span> <span style="color: #66cc66;">&#123;</span> 
    <span style="color: #FF7700;">// There are two ways to use the lookup table... </span>
    <a href="/index.php?title=integer"><span style="color: #007700;font-weight: bold; text-decoration: none;">integer</span></a> Col; 
    <a href="/index.php?title=integer"><span style="color: #007700;font-weight: bold; text-decoration: none;">integer</span></a> Row; 
    <a href="/index.php?title=if"><span style="color: #0000FF;font-weight: bold; text-decoration: none;">if</span></a> <span style="color: #66cc66;">&#40;</span>index1 <span style="color: #66cc66;">&gt;=</span> index2<span style="color: #66cc66;">&#41;</span> <span style="color: #66cc66;">&#123;</span> 
        <span style="color: #FF7700;">// In this case, the row is the index of the first character: </span>
        Row <span style="color: #66cc66;">=</span> index1; 
        <span style="color: #FF7700;">// And the col is the index of the second character (x2) </span>
        Col <span style="color: #66cc66;">=</span> index2 <span style="color: #66cc66;">*</span> <span style="color: #cc66cc;">2</span>; 
    <span style="color: #66cc66;">&#125;</span> 
    <a href="/index.php?title=else"><span style="color: #0000FF;font-weight: bold; text-decoration: none;">else</span></a> <span style="color: #66cc66;">&#123;</span> <span style="color: #FF7700;">// Index1 &lt; Index2 </span>
        <span style="color: #FF7700;">// In this case, the row is the index of the second character: </span>
        Row <span style="color: #66cc66;">=</span> index2; 
        <span style="color: #FF7700;">// And the col is the index of the first character, x2, offset by 1. </span>
        Col <span style="color: #66cc66;">=</span> index1 <span style="color: #66cc66;">*</span> <span style="color: #cc66cc;">2</span> <span style="color: #66cc66;">+</span> <span style="color: #cc66cc;">1</span>; 
    <span style="color: #66cc66;">&#125;</span> 
    <a href="/index.php?title=return"><span style="color: #0000FF;font-weight: bold; text-decoration: none;">return</span></a> <span style="color: #66cc66;">&lt;</span>Col, Row, <span style="color: #cc66cc;">0</span><span style="color: #66cc66;">&gt;</span>; 
<span style="color: #66cc66;">&#125;</span> 
