					function validation1()
					{	
						ERROR=false;
						if(!ERROR)
						if((window.document.frmcal.no_of_bedrooms.value=="")&&(window.document.frmcal.from1.value=="")&&(window.document.frmcal.to1.value=="")&&(window.document.frmcal.AreaLoc.value=="0")&&(window.document.frmcal.input.value==""))
						{	
							alert('Please Provide at least one field ')
							window.document.frmcal.no_of_bedrooms.focus();
							ERROR=true;
							
						 }
						 if(!ERROR){if(!checknumber("document.frmcal.from1","Please provide numeric value .")){ERROR=true;}}	
						 if(!ERROR){if(!checknumber("document.frmcal.to1","Please provide numeric value .")){ERROR=true;}}	
						 
						 if(!ERROR)
						 if((parseInt(window.document.frmcal.from1.value)) > (parseInt(window.document.frmcal.to1.value)))
							{	
							alert('Provide Price range from minimum to maximum  ')
							window.document.frmcal.from1.focus();
							ERROR=true;
							}
						 
						if(!ERROR)
							{
							window.document.frmcal.action="apartments_lst.asp"
							window.document.frmcal.submit();
							}
					}
