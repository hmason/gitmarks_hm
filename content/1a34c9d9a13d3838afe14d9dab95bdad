<script type="text/javascript">
//global vars for js===========================================
var posted_by_user_id = "21069";
var posted_by_username = "yahoo";
var video_guid = "8D6FC";
var media_id = "242682";
//=============================================================
function videoDelete() {
	if (confirm("Are you sure delete the video?")){
		var url = "/index.php?area=ajax&cmd=deleteVideo";
		$.post(url, {guid:video_guid}, function(data){
			if (data) {									   
				alert(data);	
			} else {
				window.location = "/videos/" + posted_by_username;
			}
		});
	}
}

function setCookie(c_name,value,expiredays)
{
var exdate=new Date();
exdate.setDate(exdate.getDate()+expiredays);
document.cookie=c_name+ "=" +escape(value)+
((expiredays==null) ? "" : ";expires="+exdate.toUTCString());
}

$(window).bind("beforeunload", function(){

        setCookie("ref_vid","8D6FC", 1);

});

function showAddTags() {
	if (logged_in) {
		$(".addtags_button").hide();
		$(".addtags_form").show();	
	} else {
		showLogin();
	}
}

function saveTags() {
	var tag_string = $("input[name='tag_string']").val();
	if (tag_string) {
		$(".addtags_loading").show();	
		$(".addtags_form").hide();
		var url = "/index.php?area=ajax&cmd=addTag";
		$.post(url, {guid:video_guid, tag_string:tag_string}, function(data){
			if (data) {
				alert(data);	
			} else {
				$(".addtags_loading").hide();	
				$(".addtags_button").show();
				$("input[name='tag_string']").val("");
				updateTags();
			}
		});
	} else {
		alert("please enter you tags!");	
	}
}

function updateTags() {
	$(".video_tags").html('<img src="/UI/images/loading.gif" alt="loading ..." />');
	var url="/index.php?area=ajax&cmd=getTags";
	$.post(url, {guid:video_guid}, function(data){
		$(".video_tags").html(data);
	});
}
</script>
