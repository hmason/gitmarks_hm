<script type="text/javascript">
$('#get_email').mouseenter(function() {
	$("#get_email_content").slideDown();
}).mouseleave(function() {
	$("#get_email_content").slideUp();
});

sampleText($("input[name='email_address']"), "insert your email");
function checkEmail(inputvalue){	
    var pattern=/^([a-zA-Z0-9_.-])+@([a-zA-Z0-9_.-])+\.([a-zA-Z])+([a-zA-Z])+/;
    if(pattern.test(inputvalue)){         
		return true;
    }else{   
		return false;
    }
}
function addEmailSubscription() {
	var sub_email = $("input[name='email_address']").val();
	if(!checkEmail(sub_email)) {
		alert("Please insert a valid email address");
		return 0;
	}

	//$(".uploading_info_foot").addClass("uploading_loading");
	$("#get_email_s1").slideUp();
	$("#get_email_s2").slideDown();
	var url = "/index.php?area=ajax&cmd=addEmailSubscription";
	$.post(url, {sub_email:sub_email, sub_user_id:21069}, function(data){
		//$(".uploading_info_foot").removeClass("uploading_loading");
		
		if (data) {
			$("#get_email_s2").slideUp();
			$("#get_email_s3").slideDown();
		}
		//$(".uploading_info").slideUp();
	});
}
</script>
