var expandArray = new Array();
var baseString  = new Array();
baseString["ecl"]="#";
baseString["rol"]="Lookup Repair #";
baseString["pol"]="Lookup Part Order #";
function formFocus(field,string,base,links){
	clearInput(field,string);
	if(base=="ecl"){
		if(document.getElementById("ecl_systemSelect").style.display=="none"){
			flipLinks(base,links,"form");
			document.getElementById("ecl_systemSelect").style.display="block";
			document.getElementById("ecl_langSelect").style.display="";
			document.getElementById("ecl_sub").style.display="";
		}
	}
	else{
		if(document.getElementById(base+"_zip").style.display=="none"){
			flipLinks(base,links,"form");
			document.getElementById(base+"_zip").style.display="";
			document.getElementById(base+"_sub").style.display="";
		}
	}
}
function formBlur(field,string,base){
	fillInput(field, string);
 	if(field.value==string){
		adjustClass(base,-1);
		if(base=="ecl"){
			document.getElementById("ecl_systemSelect").style.display="none";
			document.getElementById("ecl_langSelect").style.display="none";
			document.getElementById(base+"_sub").style.display="none";
		}
		else{
			document.getElementById(base+"_zip").style.display="none";
			document.getElementById(base+"_sub").style.display="none";
		}
	} 
}
	
function adjustClass(base,updown){
	if(expandArray[base]==null){
		expandArray[base]=0;
	}
	expandArray[base]+=updown;
	if(expandArray[base]==0){
		document.getElementById(base).setAttribute("class","complex");
		document.getElementById(base).setAttribute("className","complex");
	}
	else{
		document.getElementById(base).setAttribute("class","complex_expanded");
		document.getElementById(base).setAttribute("className","complex_expanded");
	}
}
function clearInput(targ, string){
	if(targ.value == string){
		targ.value = "";
	}
}

function fillInput(targ, string, errorcodeDiv){
	var out = "#"; 
	var add = "";
	var temp = "" + targ.value;
	while (temp.indexOf(out)>-1) {
		pos = temp.indexOf(out);
		temp = "" + (temp.substring(0, pos) + add + 
		temp.substring((pos + out.length), temp.length));
	}
	targ.value = temp;
	if(targ.value == ""){
		targ.value = string;
	}
	if(errorcodeDiv!=null&&errorcodeDiv!=''){
		document.getElementById(errorcodeDiv).style.display = "none";
	}
}

function formValidator(form,divname){
	var tempVal = form.error_code.value.replace(/-/g,'');
	if(form.error_code.value == "#" || form.error_code.value == ""){
		document.getElementById(divname).innerHTML = "<font color=red>Please enter an error code</font><br>";
		document.getElementById(divname).style.display = "inline";
		return (false);		
	}
	else if(isNaN(tempVal)){
		document.getElementById(divname).innerHTML = "<font color=red>Numerical codes only, please</font><br>";
		document.getElementById(divname).style.display = "inline";
		form.error_code.focus();
		return (false);	
	}
	else if(form.system.value == "sys"){
		document.getElementById(divname).innerHTML = "<font color=red>Please select a system</font><br>";
		document.getElementById(divname).style.display = "inline";
		form.system.focus();
		return (false);	
	}
	else{
		return (true);
	}
}

function isnum(targ,val,divname){
	var out = "#"; 
	var add = "";
	var temp = "" + val.replace(/^\s+|\s+$/g, '');
	//temp = temp.replace(/^\s+|\s+$/g, '');
	while (temp.indexOf(out)>-1) {
		pos = temp.indexOf(out);
		temp = "" + (temp.substring(0, pos) + add + 
		temp.substring((pos + out.length), temp.length));
	}
	targ.value = temp;
	var lastval = val.substring(val.length-1);
	if(isNaN(lastval) && lastval!="-"){
		document.getElementById(divname).innerHTML = "<font color=red>Numerical codes only, please</font><br>";
		document.getElementById(divname).style.display = "inline";
		var tstLgth = targ.value.length -1;
		targ.value=targ.value.substring(0,tstLgth);
	}
	else{
		document.getElementById(divname).style.display = "none";	 
	}
}
function flipLinks(base,count,src){
	var mode = (document.getElementById(base).className == "complex_expanded"?"none":"inline");
	if(mode=="none"){//Double check to see that the links are actually visible; the form could be holding it in _expanded
		if(document.getElementById(base+"_link"+count).style.display=="none") mode = "inline";
	}
	if(src=="form") mode="inline";
	
	for(var i=1;i<=count;i++){
		document.getElementById(base+"_link"+i).style.display=mode;
	}
	
	if(mode=="inline"){
		adjustClass(base,1);
	}
	else{
		adjustClass(base,1-expandArray[base]);
		clearInput(document.getElementById(base+"_num"),baseString[base]);
		formBlur(document.getElementById(base+"_num"),baseString[base],base);
	}
}
-->
