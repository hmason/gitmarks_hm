        completedStage: function() {},
        metrics: 0,
        strings: {},
        chars: {
            EOL: String.fromCharCode(0x0A),
            SQUOTE: String.fromCharCode(0x27),
            DQUOTE: String.fromCharCode(0x22),
            BACKSLASH: String.fromCharCode(0x5C),
            YEN: String.fromCharCode(0xA5)
        }
    }


</script>




