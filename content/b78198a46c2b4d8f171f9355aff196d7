<script type="text/javascript" src="http://wwwimages.adobe.com/www.adobe.com/ubi/template/identity/adobe/Modal.js"></script>
<script type="text/javascript">
adobe.link("adobe/Pane/screen.css");
document.observe("dom:loaded", function() {	
	var modallinks = $$("a[rel=modal]");
	modallinks.invoke("modalize");
	modallinks.invoke("observe","click", function(event) {
		var url = window.location.host + window.location.pathname;		
	});
});
var	ie = adobe.hostEnv.ie;
document.observe("carousel:initialized", function() {	
	
	$$(".carousel-item a[rel=modal]").each(function(modal,index) {
		if(!ie) { 
			modal.modalize();
		} else {
			modal.addClassName("modalized").setAttribute("id","modal-"+index);	
		}
	});
});

document.observe("carousel:startrevolve", function() {
	if(ie) { reModalize.delay(0.1);	}
});

function reModalize() {
	
	var modallinks = $$('.carousel-item a[rel=modal]');	
		
	modallinks.each(function(link,index){
		if(!link.hasClassName("modalized") && link.up(3).style.visibility != "hidden") {
			if(link.id != "") return;	// for IE6 (?? if it has the ID then already been modalized)
			link.addClassName("modalized").setAttribute("id","modal-"+index); // modal object removed from link, need to re-add
			link.modalize();
		}
	});
}

</script>




<script type="text/javascript">
adobe.link("product.cs4/tree/white.css", {media:"screen"});
adobe.link("product.cs4/tree/print.css", {media:"print"});
adobe.use("adobe.u");
adobe.use("adobe.Tree");
Event.observe(window, "load", function() {
	$$(".dyn-treelist").collect(function(el) {
		var param = adobe.u.getSearchParam(el.identify()),
		params = (param) ? param.split(",").invoke("toInt") : [];
		new adobe.Tree(el, {
			show: params,
			style_button_closed: "tree-branch-closed",
			style_button_opened: "tree-branch-opened",
			style_uninitialized: "dyn-treelist",
			style_initialized: "treelist"
		}, 
		{
			toggle_all: "Alt+click to show/hide all features"
		});
	});
});
</script>

