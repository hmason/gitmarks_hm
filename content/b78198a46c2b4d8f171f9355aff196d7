		postDateMonth1 = changeMonth(postDateMonth1);
		postDateMonth2 = changeMonth(postDateMonth2);
		postDateMonth3 = changeMonth(postDateMonth3);
		postDateMonth4 = changeMonth(postDateMonth4);
		postDateMonth5 = changeMonth(postDateMonth5);
		
		function changeMonth(monthVal) {
			if (monthVal == "01") {
				return "Jan";
			}
			else if (monthVal == "02") {
				return "Feb";
			}
			else if (monthVal == "03") {
				return "Mar";
			}
			else if (monthVal == "04") {
				return "Apr";
			}
			else if (monthVal == "05") {
				return "May";
			}
			else if (monthVal == "06") {
				return "Jun";
			}
			else if (monthVal == "07") {
				return "Jul";
			}
			else if (monthVal == "08") {
				return "Aug";
			}
			else if (monthVal == "09") {
				return "Sep";
			}
			else if (monthVal == "10") {
				return "Oct";
			}
			else if (monthVal == "11") {
				return "Nov";
			}
			else if (monthVal == "12") {
				return "Dec";
			}
		}
			
		postDate1 = postDateDay1 + " " + postDateMonth1 + " " + postDateYear1;
		postDate2 = postDateDay2 + " " + postDateMonth2 + " " + postDateYear2;
		postDate3 = postDateDay3 + " " + postDateMonth3 + " " + postDateYear3;
		postDate4 = postDateDay4 + " " + postDateMonth4 + " " + postDateYear4;
		postDate5 = postDateDay5 + " " + postDateMonth5 + " " + postDateYear5;
		
		postBlogHeader1 = "<h5><a href=\"" + postURL1 + "\" target=\"_blank\">" + postHeadline1 + "</a></h5>";
		postBlogHeader2 = "<h5><a href=\"" + postURL2 + "\" target=\"_blank\">" + postHeadline2 + "</a></h5>";
		postBlogHeader3 = "<h5><a href=\"" + postURL3 + "\" target=\"_blank\">" + postHeadline3 + "</a></h5>";
		postBlogHeader4 = "<h5><a href=\"" + postURL4 + "\" target=\"_blank\">" + postHeadline4 + "</a></h5>";
		postBlogHeader5 = "<h5><a href=\"" + postURL5 + "\" target=\"_blank\">" + postHeadline5 + "</a></h5>";
	
		postBlogDate1 = "<p class=\"stamp-date\">" + postDate1 + "</p>";
		postBlogDate2 = "<p class=\"stamp-date\">" + postDate2 + "</p>";
		postBlogDate3 = "<p class=\"stamp-date\">" + postDate3 + "</p>";
		postBlogDate4 = "<p class=\"stamp-date\">" + postDate4 + "</p>";
		postBlogDate5 = "<p class=\"stamp-date\">" + postDate5 + "</p>";
		
		postBlogMoreLink = "<p><a class=\"link-more\" href=\"http://blogs.adobe.com/flashplatform/\" target=\"_blank\">Read more</a></p>";
		
		$('blog_postings').insert( {'top':postBlogMoreLink} );
		$('blog_postings').insert( {'top':postBlogDate5} );
		$('blog_postings').insert( {'top':postBlogHeader5} );
		$('blog_postings').insert( {'top':postBlogDate4} );
		$('blog_postings').insert( {'top':postBlogHeader4} );
		$('blog_postings').insert( {'top':postBlogDate3} );
		$('blog_postings').insert( {'top':postBlogHeader3} );
		$('blog_postings').insert( {'top':postBlogDate2} );
		$('blog_postings').insert( {'top':postBlogHeader2} );
		$('blog_postings').insert( {'top':postBlogDate1} );
		$('blog_postings').insert( {'top':postBlogHeader1} );
	}
		
	window.onload=function LoadXMLFile() {
		var url = "/cfusion/ajax/index.cfm?whitelist=flashplatform&target=http://blogs.adobe.com/flashplatform/feed/atom";
		Spry.Utils.loadURL("GET", url, true, ProcessXMLFile);
	}
