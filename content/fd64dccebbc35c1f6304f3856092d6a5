	<script type="text/javascript" language="javascript">
	// <![CDATA[
	
	//default search variables sent via URL to ajax_jobs.asp.
	var searchmode = 'browse';
	var level = 'pro';
	var searchkeyword = '';
	var searchzipcode = '';
	var searchdistance = '20';
	var searchcategory = 0;
	var searchstate = 0;
	var searchcountry = 0;
	var specialties_max_length = 0;
	var locations_max_length = 0;
	var first_time = true;
	var currentSearchURL = '';
	var criteriaSelected = false;
	
	var searchForm = '';

	var boxSliding = false;
	
	function searchtypeSet(elemnt){
		if(level!=elemnt){
			level=elemnt;
			document.getElementById('proSidebar').className = 'hide';		
			document.getElementById('internSidebar').className = 'hide';		
			document.getElementById('probonoSidebar').className = 'hide';		
			document.getElementById('proLink').className = 'Tab';		
			document.getElementById('internLink').className = 'Tab';		
			document.getElementById('probonoLink').className = 'Tab';		
			
			document.getElementById(elemnt+'Link').className = 'activeTab';		
			document.getElementById(elemnt+'Sidebar').className = 'show resource';		
			
			searchajax();
		}
	}
	
	function setCategory(newCat,newCatText)
	{
		document.getElementById('categoryLink').innerHTML = newCatText;
		searchcategory = newCat;
		criteriaSelected=true;
		$('ul#categoryList').slideUp('fast');
		searchajax();
	}
		
	function setState(newState,newStateText)
	{
	    searchcountry=0;
		searchstate = newState;
		criteriaSelected=true;
		document.getElementById('stateLink').innerHTML = newStateText;
		$('ul#stateList').slideUp('fast');
		searchajax();
	}

	function setCountry(newCountry,newCountryText)
	{
	    searchstate=0;
		searchcountry = newCountry;
		criteriaSelected=true;
		document.getElementById('stateLink').innerHTML = newCountryText;
		$('ul#stateList').slideUp('fast');
		searchajax();
	}
	
	function clearLocationFilter()
	{
	    searchstate=0;
	    searchcountry=0;
	    document.getElementById('stateLink').innerHTML = 'all locations';
	    searchajax();
	}

	function setDistance(newDistance)
	{
		searchdistance = newDistance;
		document.getElementById('distanceLink').innerHTML = newDistance + ' Miles';
		criteriaSelected=true;
		$('ul#distanceList').slideUp('fast');
	}
	
	function keywordSearchSubmit()
	{
		searchForm = 'keyword';
		searchkeyword =  document.getElementById('keywordinput').value;
		if (searchkeyword != '')
		{
			criteriaSelected=true;
			document.getElementById('zipcodeinput').value='';
			setDistance('20');
		    searchstate=0;
		    searchcountry=0;
		    searchcategory=0;
		    searchajax();
		}
	}
	
		
	function distanceSearchSubmit()
	{
		//clear the keyword first
		document.getElementById('keywordinput').value="";
		searchkeyword='';
		
		searchForm = 'distance';
		searchzipcode = document.getElementById('zipcodeinput').value;
		criteriaSelected=true;
		
		searchajax();
	}

	
    function enterKeyNotPressed(e,identifier)
    {
       var key;
       if(window.event)
            key = window.event.keyCode;     //IE
       else
            key = e.which;                  //firefox
       if (key==13 || key==3)            // on the mac the numeric keypad enter = 3
       {
       		if (identifier==1)
	           keywordSearchSubmit();
	        else
	        	distanceSearchSubmit();
       }
       return(key != 13 && key != 3);
    }
	
	
	function closeAll()
	{
		$('ul#distanceList').css("display", 'none');
		$('ul#categoryList').css("display", 'none');
		$('ul#stateList').css("display", 'none');
	}

	function loadDefaultList()
	{		
		searchstate=0;
		searchcountry=0;
		searchcategory=0;
		if (criteriaSelected)
			searchajax();		
	}
		
	function searchbrowse()
	{
		closeAll();
		if(searchmode == 'browse')
		{
			searchmode = 'search';
			document.getElementById('keywordsearch').className = 'show';
			document.getElementById('distancesearch').className = 'show';
			document.getElementById('browsesentence').className = 'hide';
			document.getElementById('searchbrowseLink').innerHTML = '';	
			fadeIn('keywordsearch', 0);
			fadeIn('distancesearch', 0);
			document.getElementById('searchbrowseLink').innerHTML = '&laquo; Back to browse mode';
			loadDefaultList();	
		}
		else 
		{
			searchmode = 'browse';
			document.getElementById('keywordsearch').className = 'hide';
			document.getElementById('distancesearch').className = 'hide';
			document.getElementById('browsesentence').className = 'show';
			fadeIn('browsesentence', 0);
			document.getElementById('searchbrowseLink').innerHTML = 'Search';	
			loadDefaultList();	
		}
	}
	
	function searchajax()
	{
		document.getElementById('joblist').className = 'halfAlpha';
		//$('#loadingCell').slideDown('medium'); 
		document.getElementById('loadingCell').className = 'show';
		//fadeIn('loadingCell', 0);

		//document.getElementById('joblist').innerHTML = '<table cellpadding="0" cellspacing="0" border="0"><tr><td><span id="loadinglist">Loading List<'+'/span><'+'/td><'+'/tr><'+'/table>';
		//alert(document.getElementById('joblist').innerHTML);
				
		
		
		var baseURL = '../includes/callbacks/jobs_search_results.asp?package_to_filter_on=1&package_filter_type=true&';
		searchURL = ''
		if(searchmode=='browse')
		{
		    if (searchcategory != 0)
			    searchURL += 'specialty=' + searchcategory;//+'&state='+searchstate;
			if (searchstate != 0)
			{
			    if (searchcategory != 0)
			        searchURL += '&'
			    searchURL += 'state=' + searchstate
			}
			if (searchcountry != 0)
			{
			    if (searchcategory != 0)
			        searchURL += '&'
			    searchURL += 'country=' + searchcountry
			}
		} 
		else
		{
			if(searchForm == 'distance')
			{
			    searchURL += 'zip='+searchzipcode+'&distance_in_miles='+searchdistance;
			} 
			else 
			{
			    searchURL += 'keywords='+searchkeyword;
			}
		}
		currentSearchURL=searchURL;
		searchURL=baseURL + searchURL;
		//alert(searchURL);
		
		if (window.XMLHttpRequest)
		{
			req = new XMLHttpRequest();
			req.onreadystatechange = searchreturn;
			req.open("GET", searchURL);
			req.send(null);	
		} 
		else if (window.ActiveXObject)
		{
			req = new ActiveXObject("Microsoft.XMLHTTP");
			if (req)
			{
				req.onreadystatechange = searchreturn;
				req.open("GET", searchURL);
				req.send();

			}
		}
	}

	function searchreturn(){
		if (req.readyState == 4)
		{
			if (req.status == 200)
			{
				document.getElementById('loadingCell').className = 'hide';
				//$('#loadingCell').slideUp('fast'); 
				var response_json = jQuery.parseJSON(req.responseText);
				
				var specialties = response_json.specialties
				var jobs = response_json.jobs
				var countries = response_json.countries
				var states = response_json.states
				
			    var job, specialty, country, state, i;
			    //alert(specialties.length);
			    //alert(countries.length);
			    //alert(states.length);
			    //alert(jobs.length);
			    var current_locations_length = states.length + countries.length;
			    var current_specialties_length = specialties.length;
			    if (first_time)
			    {
			        first_time=false;
			        specialties_max_length = current_specialties_length;
			        locations_max_length = current_locations_length;
			    }
			    
			    var jobs_container = $("#joblist");
			    jobs_container.empty();
			    
			    if (jobs.length==0)
			    	$("#noResultsMessage").show();
			    else
			    	$("#noResultsMessage").hide();
			    
			    
			    for (i=0;i<jobs.length;i++)
			    {
			        var job_div = document.createElement('div');
			        var location_div = document.createElement('div');
			        var position_div = document.createElement('div');
			        var date_div = document.createElement('div');
			        var company_span = document.createElement('span');
			        var title_link = document.createElement('a');
			        var br_separator = document.createElement('br');
			        			        
			        job_div.className='individuallisting';
			        location_div.className='location';
			        location_div.innerHTML=jobs[i].location;

			        position_div.className='position';
			        title_link.setAttribute('href','job_details.asp?job_id=' + jobs[i].id + '&' + currentSearchURL);
			        title_link.innerHTML=jobs[i].title;
			        position_div.appendChild(title_link);
    			    company_span.className='company';
			        company_span.innerHTML=jobs[i].company;
			        position_div.appendChild(company_span);

                    date_div.className='date';
                    date_div.innerHTML=jobs[i].date_posted;
                    
                    br_separator.className='clear';
			        
			        job_div.appendChild(location_div);			        
			        job_div.appendChild(position_div);
			        job_div.appendChild(date_div);
			        job_div.appendChild(br_separator);
			        
			        jobs_container.append(job_div);
			        			        			        
			    }
								
				document.getElementById('joblist').className = '';
				
				//fadeIn('joblist', 50);

                //those dropdowns are hidden, so no need to update?
		        if(searchmode == 'browse')
		        {
        				
			            //update the category list
			            var specialty_li, specialties_container, specialty_link
			            specialties_container = document.getElementById("categoryList");
			            specialties_container.innerHTML="";

				        //insert default choice if there is already a specialty filter
				        if (searchcategory >0)
				        {
		                    specialty_li = document.createElement('li');		
				            specialty_link = document.createElement('a');
				            specialty_link.setAttribute('href','#');
				            specialty_link.setAttribute('onClick','setCategory(0,\'all disciplines\');return false;');
				            specialty_li.appendChild(specialty_link);
				            specialty_link.innerHTML='all disciplines';
				            specialties_container.insertBefore(specialty_li,specialties_container[1])
				        }

				        for (i=0;i<specialties.length;i++)
				        {
		                    specialty_li = document.createElement('li');		
				            specialty_link = document.createElement('a');
				            specialty_link.setAttribute('href','#');
				            specialty_link.setAttribute('onClick','setCategory(' + specialties[i].id + ',\'' + specialties[i].name + '\');return false;');
				            specialty_li.appendChild(specialty_link);
				            specialty_link.innerHTML=specialties[i].name;
				            specialties_container.insertBefore(specialty_li,specialties_container[i+2])
				        }
        				
				        //update the states + countries list
			            var state_li, states_container, state_link
			            states_container = document.getElementById("stateList");
			            states_container.innerHTML="";

                        if ((searchstate > 0) || (searchcountry > 0)) 
                        {
		                    state_li = document.createElement('li');		
				            state_link = document.createElement('a');
				            state_link.setAttribute('href','#');
				            state_link.setAttribute('onClick','clearLocationFilter();return false;');
				            state_li.appendChild(state_link);
				            state_link.innerHTML='all locations';
				            states_container.insertBefore(state_li,states_container[1])
				        }

				        for (i=0;i<states.length;i++)
				        {
		                    state_li = document.createElement('li');		
				            state_link = document.createElement('a');
				            state_link.setAttribute('href','#');
				            state_link.setAttribute('onClick','setState(' + states[i].id + ',\'' + states[i].name + '\');return false;');
				            state_li.appendChild(state_link);
				            state_link.innerHTML=states[i].name + ' (' + states[i].count + ')';
				            states_container.insertBefore(state_li,states_container[i+2])
				        }				
        				
				        var state_offset = i+1;
				        for (i=0;i<countries.length;i++)
				        {
		                    state_li = document.createElement('li');		
				            state_link = document.createElement('a');
				            state_link.setAttribute('href','#');
				            state_link.setAttribute('onClick','setCountry(' + countries[i].id + ',\'' + countries[i].name + '\');return false;');
				            state_li.appendChild(state_link);
				            state_link.innerHTML=countries[i].name + ' (' + countries[i].count + ')';
				            states_container.insertBefore(state_li,states_container[i+state_offset])
				        }				
	            }

				currentProc = "";
			}
		}
	}	
	
	
	function init()
	{
		$('#distanceLink').click(function () { 
			boxSliding = true; 
			$('ul#distanceList').slideToggle('medium', function(){ boxSliding = false; } ); 
		});

		$('#categoryLink').click(function () { 
			boxSliding = true;
			$('ul#categoryList').slideToggle('fast', function(){ boxSliding = false; } ); 
			var pos = $('#categoryLink').position();
			$('ul#categoryList').css("top", (pos.top+27) + 'px');
			$('ul#categoryList').css("left", pos.left + 'px');
		});
		
		$('#stateLink').click(function () { 
			boxSliding = true;
			$('ul#stateList').slideToggle('fast', function(){ boxSliding = false; } ); 
			var pos = $('#stateLink').position();
			$('ul#stateList').css("top", (pos.top+27) + 'px');
			$('ul#stateList').css("left", pos.left + 'px');
		});
		
		$(window).resize(function() {
		  closeAll();
		});
		
		//the first load is via server side
		//searchajax();
	}
	
		
	/*
	function click(e) { //3.0
		if (document.all) {
			if (event.button == 2 | | event.button == 3 | | event.button == 6 | | event.button == 7) {
				alert(message);
				return false;
			}
		}
		if (document.layers) {
			if (e.which == 3) {
				alert(message);
				return false;
			}
		}
	}
	if (document.layers) {
		document.captureEvents(Event.MOUSEDOWN);
	}
	document.onmousedown=click;
	*/
	$(document).click(
		//function(){ alert('here?'); }
		//function(){  if(boxSliding == false){ closeAll(); }  }
		function(){  if(boxSliding == false){ closeAll(); }  }
	);
	
	window.onload = init;
	
	// ]]>
	</script>


