		<script type="text/javascript" charset="utf-8" src="/javascripts/jquery-1.2.3.min.js"></script>
		<script type="text/javascript" charset="utf-8">
			var recenvis = "<div style='margin-top: 20px;' id='twittercomments' class='rbox homefeatured'><div class='boxchild'><div id=\"recvisweb\"><div class=\"keyval\"><div class=\"key fl\">Recently Visited :</div><div class=\"value fl\"><div id='recvisint'></div></div><div class='cb'></div></div></div></div></div>";
			var totalcount = 0;
			$('#resvistemp').after(recenvis);
			try{DD_roundies.addRule('.homefeatured', '5px');DD_roundies.addRule('.homefeatured .boxchild', '4px');}catch(ex){}
			// $('').hide();
			function poll(){
				$.get('/pages/recent_visited', function(data){
					totalcount += 1;
					$('.homefeatured').show();
					if (data != $('#recvisint').html()){
						$('#recvisint').fadeOut('slow', function(){$('#recvisint').html(data);$('#recvisint').fadeIn('slow');});
					}
				});
				if (totalcount < 300) {
					setTimeout("poll()", 5000);
				}
			}
			poll();
			$.get('/pages/hit_page_stats?url=/', function(data){});
			$('a').click(function(){
				var id = null, elem = $(this);
				while(id == null) {id=elem.attr('id'); elem = elem.parent();}
				$.get("/pages/stats?from_url=" + escape(window.location.href) + "&to_url=" + escape($(this).attr('href')) + "&htmlid="+id, function(data){});
			});
		</script>
		
