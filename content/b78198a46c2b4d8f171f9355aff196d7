	function ProcessXMLFile(req) {
		// This block is necessary for IE because it doesn't create
		// an XML DOM (responseXML) when loading files off disk via file://.
	
		var doc = req.xhRequest.responseXML;
		if (!doc || !doc.firstChild) {
			doc = Spry.Utils.stringToXMLDoc(req.xhRequest.responseText);
			if (!doc || !doc.firstChild) {
				alert("Failed to get XML document DOM!");
				return;
			}
		}
	
		// Convert the XML DOM document to a JS object.
		var str = "";
		var xml = Spry.XML.documentToObject(doc);
		
		postHeadline1 = xml.feed.entry[0].title._value();
		postHeadline2 = xml.feed.entry[1].title._value();
		postHeadline3 = xml.feed.entry[2].title._value();
		postHeadline4 = xml.feed.entry[3].title._value();
		postHeadline5 = xml.feed.entry[4].title._value();

		postURL1 = xml.feed.entry[0].content["@xml:base"];
		postURL2 = xml.feed.entry[1].content["@xml:base"];
		postURL3 = xml.feed.entry[2].content["@xml:base"];
		postURL4 = xml.feed.entry[3].content["@xml:base"];
		postURL5 = xml.feed.entry[4].content["@xml:base"];
		
		postDate1 = xml.feed.entry[0].published._value();
		postDate2 = xml.feed.entry[1].published._value();
		postDate3 = xml.feed.entry[2].published._value();
		postDate4 = xml.feed.entry[3].published._value();
		postDate5 = xml.feed.entry[4].published._value();
		
		postDateYear1 = postDate1.slice(0, 4);
		postDateYear2 = postDate2.slice(0, 4);
		postDateYear3 = postDate3.slice(0, 4);
		postDateYear4 = postDate4.slice(0, 4);
		postDateYear5 = postDate5.slice(0, 4);
		
		postDateMonth1 = postDate1.slice(5, 7);
		postDateMonth2 = postDate2.slice(5, 7);
		postDateMonth3 = postDate3.slice(5, 7);
		postDateMonth4 = postDate4.slice(5, 7);
