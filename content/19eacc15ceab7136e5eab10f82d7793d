<script type="text/javascript">
$j(document).ready(function() {
// get the comment ratings for this page/user
$j.getJSON('/ajax/json-comment-ratings.php?sid=20101025/16375311580', '', function(data) {
ratings = data.ratings;
var r;
var divId;
var divBody;
for (var cid in ratings) {
r = ratings[cid];
divId = 'rc_' + cid;
divBody = '<ul>\n' +
' <li value="' + r.checked.insightful + '" class="comment_rating positive' + ((r.checked.insightful > 0) ? ' down' : '')  + '" id="ins_20101025/16375311580_' + cid + '" title="rate this comment as insightful, informative, intelligent">&nbsp;insightful&nbsp;</li>\n' +
' <li value="' + r.checked.funny      + '" class="comment_rating positive' + ((r.checked.funny > 0)      ? ' down' : '')  + '" id="fun_20101025/16375311580_' + cid + '" title="rate this comment as funny">&nbsp;funny&nbsp;</li>\n' +
' <li value="' + r.checked.abusive    + '" class="comment_rating negative' + ((r.checked.abusive > 0)    ? ' down' : '')  + '" id="rpt_20101025/16375311580_' + cid + '" title="report this comment as abusive, spam, trollish, or otherwise inappropriate">&nbsp;report&nbsp;</li>\n' +
'</ul>\n';
$j('#' + divId).html(divBody);
}
// comment rating (voting) functions
$j('.comment_rating').click(function() {
var val = $j(this).attr('value');
var id = $j(this).attr('id');
var selection = $j(this);
var chk = $j(this).hasClass('down');
var rcid = id.split("_")[2];
var data;
// if not rated, submit rating
if (!chk) {
selection.addClass('down');
var jsonurl = '/ajax/json-rate-comment.php?rc=' + id + '&crid=' + val;
//alert('request: jsonurl);
$j.getJSON(jsonurl, '', function(data) {
// on success...
if (data.crid > 0) {
// grab rating id from return value and set input value to rating id
//alert(id + " - " + selection.attr("value") + " - " + data.crid);
selection.attr('value', data.crid);
// say thanks.
$j('.rcthx').html(''); // clear out old statuses
$j('#rc_' + rcid).append('<div class="rcthx">Thanks! Saved your vote.</div>');
$j('.rcthx').fadeOut(1500);
} else {
selection.removeClass('down');
}
});
// else, if already rated, clear it
} else {
selection.removeClass('down');
var jsonurl = '/ajax/json-rate-comment.php?rc=-' + id + '&crid=' + val;
//alert('request: jsonurl);
$j.getJSON(jsonurl, '', function(data) {
// on success...
if (data.crid > 0) {
// say thanks.
$j('.rcthx').html(''); // clear out old statuses
$j('#rc_' + rcid).append('<div class="rcthx">Removed your vote.</div>');
$j('.rcthx').fadeOut(1500);
} else {
selection.addClass('down');
}
});
}
}).css({cursor:"pointer"});
});
});
</script><select id="showcomments">
