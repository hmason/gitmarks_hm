<script type="text/javascript">
function cnnQuantcast()
{
	var lb = "Time Inc News Business and Sports,CNN Money";
	var allDivs = document.getElementsByTagName("div");
	for (var i = 0; i < allDivs.length; i++) {
  		var lb2 = allDivs[i].childNodes[0];

  		if (lb2 != null && typeof(lb2 != 'undefined') && lb2.length > 0 && lb2.nodeValue.match("ADSPACE:") && lb2.nodeValue.match("336x280")) {
 	    	lb3 = lb2.nodeValue.replace(/^\s+|\s+$/g,"");
    		lb += ".Adzone." + lb3;
    		break;
    		}
      	}
	if (typeof(s_prop1)=="string" && s_prop1.length > 0) {
		lb += ",CNN Money.Category." + s_prop1;
		lb += (typeof(s_prop2)=="string" && s_prop2.length > 0) ? "." + s_prop2 :"";
	}
	lb+=(typeof(s_prop9)=="string" && s_prop9.length > 0) ? ",CNN Money.Special Features." + s_prop9:"";
	return lb;
}
_qoptions={
	qacct:"p-5dyPa639IrgIw",
	labels:cnnQuantcast()
}
</script>
