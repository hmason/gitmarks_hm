    <script type="text/javascript">
        function setWikiCommentTextBoxFocus()
        {
            $('#Comment').focus();
            return false;
        }

        function wikiSearch() {
            var $textbox = $('#searchTextBox');
            if($textbox.attr('dirty') != 'true')
                return;
            var searchText = encodeURIComponent(jQuery.trim($textbox.val()));
            if(searchText != null & searchText.length > 0)
                window.location.href = 'http://json.codeplex.com/Wiki/Search.aspx' + '?tab=Home&SearchText=' + searchText;
        }

    </script>

