<script language=JavaScript1.2>
var errors = "";

function isEmailAddressAddr(trackingId){
  var result = false
  var theStr = new String(trackingId)
  var index = theStr.indexOf("@");
  if (index > 0)  {
    var	pindex = theStr.indexOf(".",index);
    if ((pindex	> index+1) && (theStr.length > pindex+1))
	result = true;
  }
  return result;
}

    
function validate(form){
	document.getElementById("errorDiv").style.display = "none";
	document.getElementById("errorArray").innerHTML = "";
	errors = "";
	var first2 = "";
	var last9 = "";
	document.form1.serialnumber.value = document.form1.serialnumber.value.trim();
	
	if(document.form1.serialnumber.value.length != 11){
		document.getElementById("errorArray").innerHTML += "<li><font color=red>Please enter all 11 characters of the serial number </li>";
		errors = "true";
	}
	else{
		first2 = document.form1.serialnumber.value.substring(0,2);
	}
	if(document.form1.serialnumber.value.length > 0){
		if(first2 != "LU" || document.form1.serialnumber.value.indexOf('=') != -1 || document.form1.serialnumber.value.indexOf('*') != -1 || document.form1.serialnumber.value.indexOf('[') != -1 || document.form1.serialnumber.value.indexOf(']') != -1 || document.form1.serialnumber.value.indexOf('<') != -1 || document.form1.serialnumber.value.indexOf('>') != -1 || document.form1.serialnumber.value.indexOf('$') != -1 ||  document.form1.serialnumber.value.indexOf(';') != -1 || document.form1.serialnumber.value.indexOf('{') != -1 || document.form1.serialnumber.value.indexOf('}') != -1 || document.form1.serialnumber.value.indexOf('%') != -1 || document.form1.serialnumber.value.indexOf('&') != -1){
			document.getElementById("errorArray").innerHTML += "<li><font color=red>The serial number you've entered does not appear to be for a Wii console system. Please check the number and try again </li>";
			errors = "true";
		}
	}
	if(document.form1.serialnumber.value == "LU000000000"){
			document.getElementById("errorArray").innerHTML += "<li><font color=red>The serial number you've entered does not appear to be for a Wii console system. Please check the number and try again </li>";
			errors = "true";
	}
	if(document.form1.regionUSA.checked == false && document.form1.regionCAN.checked == false){
			document.getElementById("errorArray").innerHTML += "<li><font color=red>Please select a region before submitting the form.</li>";
			errors = "true";
	}
	if(errors != ""){
		document.getElementById("errorDiv").style.display = "inline";
		window.scroll(0,250);
		return (false);
	}
	else{
 		form.poster.disabled=true;
 		form.poster.value='Please Wait';
 		return (true);
	}
}

function capSer(sn){
	tempSn = sn;
	document.form1.serialnumber.value = tempSn.toUpperCase();	
}

function replaceChars(object,entry) {
	out = " "; 
	add = "";
	temp = "" + entry;
	while (temp.indexOf(out)>-1) {
		pos = temp.indexOf(out);
		temp = "" + (temp.substring(0, pos) + add + 
		temp.substring((pos + out.length), temp.length));
	}
	temp2 = temp;
	out2 = "-"; 
	add2 = "";
	while (temp2.indexOf(out2)>-1) {
		pos= temp2.indexOf(out2);
		temp2 = "" + (temp2.substring(0, pos) + add2 + 
		temp2.substring((pos + out2.length), temp2.length));
	}
object.value = temp2;
}

function regChecker(opt){
	if(opt == "USA"){
		document.form1.regionCAN.checked = false;
		document.form1.region.value = "USA";
	}
	else{
		document.form1.regionUSA.checked = false;
		document.form1.region.value = "CAN";
	}
}

