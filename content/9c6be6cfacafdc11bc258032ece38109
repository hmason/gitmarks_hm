    function fnHide(){
      setInterval("fnHide2()", 5000);
    }
    function fnHide2(){
      var theDivA;
	  var theDivB;
	  theDivA = document.getElementById("mobile-widgetA");
	  theDivB = document.getElementById("mobile-widgetB");
	  if(theDivA.style.display != "none")
       {
            theDivA.style.display = "none";
            theDivB.style.display = "";
       }
       else
       {
            theDivA.style.display = "";
            theDivB.style.display = "none";
       }
    }

</script>
