jQuery(document).ready(function($) {
  $(".swither_header").click(function () {
    if ($("#links").is(":hidden")) $("#links").show();
    else $("#links").hide();
  });	
	$(".switcher_wrap").hover(
    function () {}, 
    function () {
      $("#links").hide();
    }
  ); 
  $(".search_select_wrap").hover(
    function () {}, 
    function () {
      $(".options").hide();
    }
  );
