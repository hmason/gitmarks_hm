<script type="text/javascript">
var setPriceSlider = function(min, max, clear) {
	if(clear) document.fire('layerednav:clear');
	$('price_selected').update('$' + min + ' to $' + max);
	slide_price.submitLayeredNav = false;
	slide_price.setValue(min, 0);
	slide_price.setValue(max, 1);
	slide_price.setValue(min, 0); //gotta do them both twice
	slide_price.submitLayeredNav = true;
	slide_price.setValue(max, 1);
}
var initPriceSlider = function() {
	$('price_selected').update('$6 to $46');
	slide_price = new Control.Slider(['price_slider_min', 'price_slider_max'], 'price_slider', {
	    range: $R(6, 46),
	    values: $R(6, 46),
	    sliderValue: [6, 46],
	    restricted: true,
	    onSlide: function(values) { 
	    	$('price_selected').update('$' + values[0] + ' to $' + values[1]);
	    },
	    onChange: function(values) {
	    	$('price_from').value = values[0];
	    	$('price_to').value = values[1];
	  		if(slide_price.submitLayeredNav) document.fire('layerednav:submit');
	    }
	});
	slide_price.submitLayeredNav = true;
}

document.observe('layerednav:load', initPriceSlider);
document.observe('layerednav:cleared', initPriceSlider);
document.observe('layerednav:disable', function() { slide_price.setDisabled(); });
document.observe('layerednav:enable', function() { slide_price.setEnabled(); });
</script><dt class="code-prescription_type" code="prescription_type">
