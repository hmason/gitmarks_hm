                  <td><script language="JavaScript">
function callsubmit(flg)
{
var e = window.event;

if(e.keyCode==13)
  if(flg==0)
	 fnGo();
  else if(flg==1)
	   	 fnSearchWithin();
 }
 function callsubmitpodcast(flg)
{
var e = window.event;

if(e.keyCode==13)
  if(flg==0)
	 fnGo();
  else if(flg==1)
	   	 fnSearchWithinPodcast();
 }
function fnGo()
{
	if(trim(document.frm.q.value)=="" )
	 {
	  alert("Specifiy the search criteria")
	 }
	 else
	 {
	 	var lolhost="0";
      
      if(lolhost != 1)
      {
         document.frm.action="http://www.learnoutloud.com/google_search.php";
         document.frm.submit();
      }
      else if(lolhost == 1)
      {
         document.frm.action="http://teach.learnoutloud.com/google_search_tol.php";
         document.frm.submit();
      }	
      
	 }

}			
function fnSearchWithin()
{
	var level='';
	var catid='';
	var id='';
	var path='http://www.learnoutloud.com';
	var cookie='';
	var surl='';
	var url="";
	if(cookie==0 && surl!='')
		url=surl;
	try{	
	searchtxt=document.getElementById("txtSearch").value;	
	searchtxt=searchtxt.replace("&","§");
	}catch(e){};
	if(level=='0')
	location.href=path+"/category.php?txtsrc="+searchtxt+"&catid="+catid+"&id="+id+"&level="+level+"&nav=L&"+url+"#go";
	if(level=='1')
	 location.href=	path+"/subcategory.php?txtsrc="+searchtxt+"&catid="+catid+"&id="+id+"&level="+level+"&nav=L&"+url+"#go";
		
}
function fnSearchWithinPodcast()
{
	var path='http://www.learnoutloud.com';
	var cookie='';
	var surl='';
	var url="";
	if(cookie==0 && surl!='')
		url=surl;
  try{		
	searchtxt=document.getElementById("txtSearch2").value;	
    searchtxt=searchtxt.replace("&","§");
	}catch(e){}; 
	location.href=	path+"/podcast_results.php?fname="+searchtxt+"&"+url+"#go";
}
</script>
