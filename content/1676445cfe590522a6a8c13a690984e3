                    <span style="color: #66cc66;">&#123;</span>
                        <span style="color: #FF7700;">//llOwnerSay(&quot;Slave Removed: &quot; + llList2String(gSlaveNames, x));</span>
                        gSlaveNames <span style="color: #66cc66;">=</span> <a href="/index.php?title=llDeleteSubList"><span style="color: #A00000;font-weight: bold; text-decoration: none;">llDeleteSubList</span></a><span style="color: #66cc66;">&#40;</span>gSlaveNames, x, x<span style="color: #66cc66;">&#41;</span>;
