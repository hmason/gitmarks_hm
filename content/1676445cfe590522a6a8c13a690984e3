ResetCharIndex<span style="color: #66cc66;">&#40;</span><span style="color: #66cc66;">&#41;</span> <span style="color: #66cc66;">&#123;</span> 
    gCharIndex  <span style="color: #66cc66;">=</span> <span style="color: #00A000;">&quot;&nbsp;!<span style="color: #FF7700;">\&quot;</span>#$%&amp;'()*+,-./0123456789:;&lt;=&gt;?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[<span style="color: #FF7700;">\\</span>]^_`&quot;</span>; 
    <span style="color: #FF7700;">// \&quot; &lt;-- Fixes LSL syntax highlighting bug. </span>
    gCharIndex <span style="color: #66cc66;">+=</span> <span style="color: #00A000;">&quot;abcdefghijklmnopqrstuvwxyz{|}~&quot;</span>; 
    gCharIndex <span style="color: #66cc66;">+=</span> <span style="color: #00A000;">&quot;<span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span><span style="color: #FF7700;">\n</span>&quot;</span>; 
