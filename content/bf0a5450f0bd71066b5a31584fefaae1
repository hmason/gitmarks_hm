	function addComm(x){var rgx=/(\d+)(\d{3})/;while(rgx.test(x)){x=x.replace(rgx,'$1'+','+'$2')};return x}
	var jcountNum = "60676";
	//jcountNum = parseInt(jcountNum.replace("<jobcount>",""))
	if(jcountNum>0){
		jcountNum = addComm(jcountNum.toString())
		if(getEl("jcount")){getEl("jcount").innerHTML = jcountNum}
		if(getEl("jcount4")){ getEl("jcount4").innerHTML = "Search "+jcountNum+" jobs<br/>in Greater Boston" }
		else if(getEl("jcount2")){ getEl("jcount2").innerHTML = "Search "+jcountNum+" jobs" }
	}
	if(getEl('bcrumb')){
		oas_bcrumb=getEl('bcrumb').innerHTML.replace(/<.*?a.*?>/gi,"").replace(/&gt;/gi,"|").replace("BOSTON.COM JOBS |","Jobs |");
	}
</script>
