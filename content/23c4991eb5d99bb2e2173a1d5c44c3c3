<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
<script type="text/javascript" src="../includes/gmap3.js"></script>
<script type="text/javascript">

var usercenter = new google.maps.LatLng(0, 0);
var icon0 = baseIcon;
var iconff = "../img/maps/ff.png";

function load() {
	var MyOptions = {
		zoom: 14,
		center: usercenter,
		mapTypeId: google.maps.MapTypeId.TERRAIN,
		scaleControl: true,
		mapTypeControl: false,
		navigationControl: true,
		navigationControlOptions: { style: google.maps.NavigationControlStyle.SMALL },
		scrollwheel:false
		}
	map = new google.maps.Map(document.getElementById("map"), MyOptions);
	bounds = new google.maps.LatLngBounds();

	var data = '<markers><marker name=\"Pawtucket / Wintertime Farmers Market\" address=\"1005 Main St., Pawtucket, RI\" lat=\"41.863125\" lng=\"-71.399414\" blurb=\"\" group=\"ff\" url=\"\"/></markers>';
	var xml = xmlParse(data);
	var markers = xml.documentElement.getElementsByTagName("marker");

	var star = new google.maps.MarkerImage("../img/maps/star.png",new google.maps.Size(18,18), new google.maps.Point(0,0), new google.maps.Point(9,9));
	var marker = new google.maps.Marker({position:usercenter, map:map, icon:star, clickable:false, zIndex:0});			
	for (var i = 0; i < markers.length; i++) {
		var name = markers[i].getAttribute("name");
		var address = markers[i].getAttribute("address");
		var blurb = markers[i].getAttribute("blurb");
		var group = markers[i].getAttribute("group");
		var url = markers[i].getAttribute("url");
		var point = new google.maps.LatLng(parseFloat(markers[i].getAttribute("lat")),parseFloat(markers[i].getAttribute("lng")));
		var marker = createMarker(point, name, address, blurb, group, url);
		bounds.extend(marker.getPosition());
	}
		
	google.maps.event.addListener(map, "click", function() { if (html) html.close(); });
	map.setCenter(bounds.getCenter());
map.setZoom(14);
	
}
</script>
