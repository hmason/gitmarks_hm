            <span style="color: #66cc66;">&#123;</span><span style="color: #FF7700;">//isn't registered yet</span>
                gSlaveNames <span style="color: #66cc66;">+=</span> data;
                gSlaveRegistered<span style="color: #66cc66;">=</span><a href="/index.php?title=Special:Search&amp;go=Go&amp;search=TRUE"><span style="color: #0000A0;font-weight: bold; text-decoration: none;">TRUE</span></a>;
                <span style="color: #FF7700;">//llOwnerSay((string)llGetListLength(gSlaveNames) + &quot; Slave(s) Recognized: &quot; + data);</span>
