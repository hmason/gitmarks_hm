				<script type="text/javascript">

                var menuTimer;
                var effectDropdown=null;
                var setInfoLeft = false;

                  document.observe("dom:loaded", function() {
                    $$('.watchMenuHover').invoke('observe', 'mouseover', expandTopMenu);
                    $$('.watchMenuHover').invoke('observe', 'mouseout', hideTopMenu);

                    $$('.subMenu').invoke('observe', 'mouseover', cancelTimers);
                    $$('.subMenu').invoke('observe', 'mouseout', hideTopMenu);

                    $("myAccountMenuSub").setAttribute("maxHeight",$("myAccountMenuSub").getHeight()+"px");
                    $("informationMenuSub").setAttribute("maxHeight",$("informationMenuSub").getHeight()+"px");
                  });

                  function expandTopMenu()
                  {
                    clearTimeout(menuTimer);
                    hideAllMenusTimer();
                    this.addClassName('hover');
                    var elem = this.readAttribute("id")+"Sub";
                    $(elem).setStyle({height:"0px"}).show();
                    if(elem == "informationMenuSub" && !setInfoLeft)
                    {
                      //$("informationMenuSub").setStyle({"left":($("informationMenuSub").cumulativeOffset().left)+"px"});
                      setInfoLeft = true;
                    }
                      
                    effectDropdown = new Effect.Morph(elem,{style:'height:'+$(elem).readAttribute("maxHeight"), duration: 0.5 });
                  }

                  function hideTopMenu()
                  {
                    menuTimer = setTimeout("hideAllMenusTimer()",501); 
                  }

                  function cancelTimers()
                  {
                    clearTimeout(menuTimer);
                  }

                  function hideAllMenusTimer()
                  {
                    if(effectDropdown)
                      effectDropdown.cancel();

                    $$(".watchMenuHover").each(function(el){
                        el.removeClassName('hover');
                    });
                    $("myAccountMenuSub").hide();
                    $("informationMenuSub").hide();
                  }

                </script>
                
    </div> 
