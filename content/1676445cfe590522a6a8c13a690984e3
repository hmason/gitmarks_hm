                gCharIndex <span style="color: #66cc66;">=</span> <a href="/index.php?title=llDeleteSubString"><span style="color: #A00000;font-weight: bold; text-decoration: none;">llDeleteSubString</span></a><span style="color: #66cc66;">&#40;</span>gCharIndex, Index1, Index1<span style="color: #66cc66;">&#41;</span>;
                gCharIndex <span style="color: #66cc66;">=</span> <a href="/index.php?title=llInsertString"><span style="color: #A00000;font-weight: bold; text-decoration: none;">llInsertString</span></a><span style="color: #66cc66;">&#40;</span>gCharIndex, Index1, Value2<span style="color: #66cc66;">&#41;</span>;
