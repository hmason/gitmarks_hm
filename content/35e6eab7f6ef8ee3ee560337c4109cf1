					$(document).ready(function(){
						if($("#bssContent div[id^='bss']").length)
							$("#content").css("min-height",0);
						else
							$("#content").css("min-height",getMaxDiv("#content div"));
						
						function getMaxDiv(selector){
							var max = 0;
							$(selector).each(function () {
								var h = parseInt($(this).height());
								if ( h > max ) {
									max = h;
								}
							});
							return max;
						}
					});
