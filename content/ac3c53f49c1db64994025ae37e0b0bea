 }  
 if(window.attachEvent) 
 window.attachEvent("onload",lpAddMonitorTag);  
 else 
 window.addEventListener("load",lpAddMonitorTag,false);
</script><!-- END LivePerson Monitor. --></td>
  </tr>
  <tr>
    <td class="bluelight"><table width="100%"  border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td width="5" height="5"><img src="http://www.learnoutloud.com/images/corner_btt_left.gif" width="5" height="5"></td>
        <td width="100%" background="http://www.learnoutloud.com/images/btt_bg.gif"><img src="http://www.learnoutloud.com/images/spacer.gif" width="1" height="1"></td>
