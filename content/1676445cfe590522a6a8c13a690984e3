            <a href="/index.php?title=llMessageLinked"><span style="color: #A00000;font-weight: bold; text-decoration: none;">llMessageLinked</span></a><span style="color: #66cc66;">&#40;</span><a href="/index.php?title=Special:Search&amp;go=Go&amp;search=LINK_THIS"><span style="color: #0000A0;font-weight: bold; text-decoration: none;">LINK_THIS</span></a>, REGISTER_SLAVE, <a href="/index.php?title=llGetScriptName"><span style="color: #A00000;font-weight: bold; text-decoration: none;">llGetScriptName</span></a><span style="color: #66cc66;">&#40;</span><span style="color: #66cc66;">&#41;</span> , <a href="/index.php?title=Special:Search&amp;go=Go&amp;search=NULL_KEY"><span style="color: #0000A0;font-weight: bold; text-decoration: none;">NULL_KEY</span></a><span style="color: #66cc66;">&#41;</span>;
