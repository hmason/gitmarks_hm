    map.addControl(new GMapTypeControl());

    var baseIcon = new GIcon();
    //baseIcon.shadow = "http://www.google.com/mapfiles/shadow50.png";
    //baseIcon.shadow = "/images/toilet_marker.png";  
    baseIcon.iconSize = new GSize(32, 32);
